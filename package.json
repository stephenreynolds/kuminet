{
  "name": "screeps",
  "version": "4.0.0",
  "description": "My Screeps AI",
  "author": "Stephen Reynolds (screeps@stephenreynolds.dev)",
  "license": "MIT",
  "homepage": "https://github.com/stephenreynolds/screeps#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenreynolds/screeps.git"
  },
  "bugs": {
    "url": "https://github.com/stephenreynolds/screeps/issues"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-sim": "rollup -c --environment DEST:sim",
    "push-dev": "rollup -c --environment DEST:dev",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "gen-docs": "typedoc --out docs src"
  },
  "devDependencies": {
    "@types/lodash": "^3.10.1",
    "@types/node": "^10.5.5",
    "@types/screeps": "^2.4.0",
    "@types/screeps-profiler": "^1.2.2",
    "git-rev-sync": "^1.12.0",
    "lodash": "^4.17.11",
    "prettier": "^1.14.0",
    "rollup": "^0.63.4",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-nodent": "^0.2.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-screeps": "^0.1.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "screeps-profiler": "^2.0.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typedoc": "^0.14.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "source-map": "~0.6.1"
  }
}
