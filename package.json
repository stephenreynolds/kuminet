{
  "name": "screeps",
  "version": "4.0.0",
  "description": "My Screeps AI",
  "author": "Stephen Reynolds (screeps@stephenreynolds.dev)",
  "license": "MIT",
  "homepage": "https://github.com/stephenreynolds/screeps#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenreynolds/screeps.git"
  },
  "bugs": {
    "url": "https://github.com/stephenreynolds/screeps/issues"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-sim": "rollup -c --environment DEST:sim",
    "push-dev": "rollup -c --environment DEST:dev",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-sim": "rollup -cw --environment DEST:sim",
    "gen-docs": "typedoc --out docs src"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/lodash": "^3.10.2",
    "@types/node": "^13.13.1",
    "@types/screeps": "^3.1.0",
    "@types/screeps-profiler": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@typescript-eslint/typescript-estree": "^3.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "git-rev-sync": "^1.12.0",
    "lodash": "^4.17.11",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-screeps": "^1.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "screeps-profiler": "^2.0.1",
    "ts-node": "^8.8.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "source-map": "~0.6.1"
  }
}
